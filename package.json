		"svelte-codemirror-editor": "^1.3.0",
		"svelte-dnd-action": "^0.9.50",
		"svelte-infinite-loading": "^1.3.8",
		"svelte-persisted-store": "^0.11.0",
		"svelte-portal": "^2.2.1"
	}
}
