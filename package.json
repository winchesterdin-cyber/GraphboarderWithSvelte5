		"vitest": "^4.0.5",
		"vitest-browser-svelte": "^2.0.1"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"better-sqlite3": "^12.4.1",
		"@codemirror/lang-javascript": "^6.2.2",
		"@lukeed/uuid": "^2.0.0",
		"@tanstack/svelte-table": "npm:tanstack-table-8-svelte-5@^0.1",
		"@urql/core": "^2.4.4",
		"@urql/svelte": "^4.0.4",
		"bootstrap-icons": "^1.8.1",
		"cm6-graphql": "^0.0.14",
		"codemirror": "^6.0.1",
		"cookie": "^0.4.1",
		"daisyui": "^5.0.0",
